<div class="row translation_container">
  <div class="col-md-3 translation_list">
    <% translation.each do |translation| %>
        <div class="translation_item">
          <div class="row">
            <div class="col-md-1">
              <%= translation[:locale] %>
            </div>
            <div class="col-md-5">
              <%= translation[:key] %>
            </div>
            <div class="col-md-6 ">
              <div class="translation_filename">
                    <%= translation[:filename].reverse %>
              </div>
            </div>
          </div>
        </div>
    <% end %>
  </div>
</div>








